{
    "extends": "airbnb",
    "ecmaFeatures": {
          "jsx": true
    },
    "env": {
        "jest": true,
        "browser": true
    },
    "rules": {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
},
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
